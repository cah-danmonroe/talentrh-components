{{#if showButtonComplete}}
  <button type="button" class="talent-button-right" data-toggle="tooltip" title="Completar Automaticamente" {{action 'findCompleteData'}}>
    {{#if searching}}
      <i class="fa fa-spinner talent-spin"></i>
    {{else}}
      <i class="fa fa-search-plus"></i>
    {{/if}}
  </button>
{{/if}}
<div class="form-group">
  {{input
    class=(if value 'form-control dirty' 'form-control')
    value=value
    required=required
    disabled=disabled
    placeholder=placeholder}}
  <i class="fa fa-check fa-icon" aria-hidden="true"></i>
  <i class="fa fa-times-circle fa-icon" aria-hidden="true"></i>
  <span class="validation-error-message">{{if errorMessage errorMessage 'CNPJ inv√°lido'}}</span>

  <label>{{placeholder}}</label>
</div>
